{
  "name": "contributors-img",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:staging": "ng build -c=staging",
    "build:prod": "ng build -c=production",
    "test": "ng test",
    "test:ci": "ng test --watch false --browsers ChromeHeadless",
    "format": "prettier --write '**/*.{ts,scss,html}'",
    "lint": "ng lint && prettier --check '**/*.{ts,scss,html}'",
    "env:local": "cat ./config/env/.env.local > .env",
    "env:dev": "cat ./config/env/.env.dev > .env",
    "env:prod": "cat ./config/env/.env.prod > .env",
    "build:all:staging": "nx run-many --projects=client,server --target=build --with-deps -c=staging",
    "build:all:prod": "nx run-many --projects=client,server --target=build --with-deps -c=production",
    "build:client": "nx build client --with-deps",
    "build:server": "nx build server --with-deps"
  },
  "private": true,
